{
  "name": "secure-firebase-proxy",
  "version": "3.4.2",
  "description": "Secure Proxy system for Firebase with SecureArmor v14.1 Enhanced Protection",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:secure": "NODE_ENV=development nodemon server.js",
    "prod": "NODE_ENV=production node server.js",
    "test": "jest --coverage",
    "test:security": "node tests/security.test.js",
    "test:auth": "node tests/auth.test.js",
    "test:api": "node tests/api.test.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "security:audit": "npm audit --production",
    "security:fix": "npm audit fix",
    "security:check": "node -e \"require('./config').validateConfig()\"",
    "deploy": "npm run security:audit && npm start",
    "clean": "rm -rf node_modules package-lock.json && npm install"
  },
  "keywords": [
    "firebase",
    "proxy",
    "security",
    "ddos-protection",
    "rate-limiting",
    "waf"
  ],
  "author": "SecureArmor Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.21.2",
    "cors": "^2.8.5",
    "helmet": "^8.0.0",
    "dotenv": "^16.4.7",
    
    "axios": "^1.7.9",
    
    "ioredis": "^5.4.2",
    
    "express-rate-limit": "^7.5.0",
    "rate-limit-redis": "^4.2.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "hpp": "^0.2.3",
    "express-validator": "^7.2.1",
    "express-useragent": "^1.0.15",
    "ua-parser-js": "^1.0.39",
    "geoip-lite": "^1.4.10",
    
    "winston": "^3.17.0",
    "morgan": "^1.10.0",
    "prom-client": "^15.1.3",
    
    "lodash": "^4.17.21",
    "uuid": "^11.0.4",
    "validator": "^13.12.0",
    "qs": "^6.13.1",
    "compression": "^1.7.5",
    "cookie-parser": "^1.4.7",
    "body-parser": "^1.20.3",
    
    "http-proxy-middleware": "^3.0.3",
    "request-ip": "^3.3.0",
    
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.16"
  },
  "devDependencies": {
    "nodemon": "^3.1.9",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "private": true,
  "overrides": {
    "semver": "^7.6.0",
    "tough-cookie": "^5.0.0",
    "got": "^14.0.0"
  }
}
